                "quantity": "1 1/2 tbs, plus extra to serve"
            },
            {
                "ingredient_name": "brown onion, finely chopped",
                "quantity": "1 small"
            },
            {
                "ingredient_name": "garlic cloves, crushed",
                "quantity": "2"
            },
            {
                "ingredient_name": "90-second white long grain rice",
                "quantity": "2 x 250g pkts"
            },
            {
                "ingredient_name": "vegetable stock cube",
                "quantity": "1, crumbled"
            },
            {
                "ingredient_name": "boiling water",
                "quantity": "500ml (2 cups)"
